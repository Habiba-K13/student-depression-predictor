    "from sklearn.metrics import roc_curve, auc\n",
    "\n",
    "# Logistic Regression\n",
    "y_proba_log = log_model.predict_proba(X_test)[:, 1]\n",
    "fpr_log, tpr_log, _ = roc_curve(y_test, y_proba_log)\n",
    "roc_auc_log = auc(fpr_log, tpr_log)\n",
